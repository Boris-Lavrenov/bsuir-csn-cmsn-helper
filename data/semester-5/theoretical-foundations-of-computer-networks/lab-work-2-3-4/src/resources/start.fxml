<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>
<AnchorPane prefHeight="55.0" prefWidth="170.0" styleClass="background" stylesheets="@theme.css"
            xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bsuir.balashenka;.Init">
    <children>
        <ComboBox fx:id="portComboBox" layoutX="10.0" layoutY="10.0" prefHeight="25.0" prefWidth="110.0"
                  promptText="Choose port"/>
        <Button fx:id="connectButton" layoutX="125.0" layoutY="10.0" mnemonicParsing="false" text="Connect!"/>
        <Text fx:id="outputText" layoutX="25.0" layoutY="50.0" strokeType="OUTSIDE" strokeWidth="0.0"
              textAlignment="CENTER" wrappingWidth="150.0"/>
    </children>
</AnchorPane>
